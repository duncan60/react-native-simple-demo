{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "react",
    "import"
  ],
  "rules": {
    "max-len": ["error", 200, 4],
    "react/jsx-boolean-value": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-console": 0,
    "consistent-return": 0,
    "global-require": 0,
    "class-methods-use-this": 0,
    "react/prefer-stateless-function": 0,
    "import/no-extraneous-dependencies":0,
    "import/no-unresolved": 0,
    "import/extensions":0
  }
}
